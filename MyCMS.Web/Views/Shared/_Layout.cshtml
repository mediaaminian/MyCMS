@using StackExchange.Profiling
<!DOCTYPE html>
<html>
<head>
    @Html.Action(MVC.MetaTag.ActionNames.Index, MVC.MetaTag.Name, new { title = ViewBag.Title, keywords = ViewBag.Keywords, description = ViewBag.Description })
    <link rel="canonical" href="@Request.Url.AbsoluteUri" />
    <meta http-equiv="Content-Language" content="fa" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    @RenderSection("metatags", false)
    <link rel="shortcut icon" href="@Links.Content.Images.fav.iefavicon_ico" type="image/x-icon">
    <link rel="shortcut icon" href="@Links.Content.Images.fav.favicon_ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="@Links.Content.Images.fav.apple_touch_icon_iphone_png" />
    <link rel="apple-touch-icon" sizes="72x72" href="@Links.Content.Images.fav.apple_touch_icon_ipad_png" />
    <link rel="apple-touch-icon" sizes="114x114" href="@Links.Content.Images.fav.apple_touch_icon_iphone4_png" />
    <link rel="apple-touch-icon" sizes="144x144" href="@Links.Content.Images.fav.apple_touch_icon_ipad3_png" />
    <link title="فید دانشنامه های سایت" href="@Url.Action(MVC.Feed.ActionNames.Posts, MVC.Feed.Name)" type="application/rss+xml" rel="alternate" />
    <link title="فید دیدگاه های سایت" href="@Url.Action(MVC.Feed.ActionNames.Comments, MVC.Feed.Name)" type="application/rss+xml" rel="alternate" />
    <meta name="designer" content="http://www.MyCMS.ir" />
    <meta name="dc.subject" content="" />
    <meta name="copyright" content="MyCMS.ir" />
    <meta name="e-mail" content="info@MyCMS.ir" />
    <meta name="distribution" content="global" />
    <meta name="google" content="notranslate" />
    <meta name="robots" content="all,index,follow" />
    <meta name="googlebot" content="all,index,follow" />
    <meta name="msnbot" content="all,index,follow" />
    <meta name="revisit" content="30 Days" />
    <meta name="expires" content="never" />
    <meta name="classification" content="" />
    <link rel="publisher" href="http://www.MyCMS.ir" />
    <meta property="og:locale" content="fa_IR" />
    <meta property="og:type" content="Website" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    @Styles.Render("~/bundles/header/css")
    @*, "~/Content/themes/start/autocompleteandanimations"*@
    <link type="text/css" href="/Content/themes/custom-theme/jquery-ui-1.10.0.custom.css" rel="stylesheet" />
    <!--[if lt IE 9]>
    <link rel="stylesheet" type="text/css" href="/Content/themes/custom-theme/jquery.ui.1.10.0.ie.css"/>
    <![endif]-->

    @*    @Styles.Render("~/Content/bootstrap/css", "~/Content/themes/start/autocompleteandanimations", "~/Content/sitecss")*@
    @RenderSection("styles", false)
    <!--[if lt IE 9]>
        <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
    <![endif]-->
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/AngularJs")

</head>
<body class="stretched device-lg" data-loader="14" ng-app="app" >
    @if (User.Identity.IsAuthenticated)
    {
        @Html.Action(MVC.Admin.Home.ActionNames.RenderDashBoard, MVC.Admin.Home.Name, new { area = "Admin" })
    }

        <!-- Document Wrapper
        ============================================= -->
        <div id="wrapper" class="clearfix">

            <!-- Top Bar
            ============================================= -->
            <div id="top-bar" class="hidden-sm hidden-xs" style="display:none;">

                <div class="container clearfix">

                    <div class="col_half nobottommargin">

                        <!-- Top Links
                        ============================================= -->
                        <div class="top-links">
                            <span style="font-family:Yekan;"> امروز چهارشنبه ، 21 خرداد 1395</span>
                        </div>
                        <!-- .top-links end -->

                    </div>

                    <div class="col_half fleft col_last nobottommargin">

                        <!-- Top Social
                        ============================================= -->
                        <div id="top-social">
                            <ul>
                                <li><a href="#" class="si-facebook"><span class="ts-icon"><i class="icon-facebook"></i></span><span class="ts-text" style="font-size:13px;margin-top:1px;">Facebook</span></a></li>
                                <li><a href="#" class="si-linkedin"><span class="ts-icon"><i class="icon-linkedin"></i></span><span class="ts-text" style="font-size:12px;margin-top:1px;">Linkedin</span></a></li>
                                <li><a href="#" class="si-instagram"><span class="ts-icon"><i class="icon-instagram2"></i></span><span class="ts-text" style="font-size:13px;margin-top:1px;">Instagram</span></a></li>
                                <li><a href="http://telegram.me/MyCMS" class="si-twitter"><span class="ts-icon"><i class="icon-email2"></i></span><span class="ts-text" style="font-size:13px;margin-top:1px;">Telegram</span></a></li>
                                <li><a href="tel:+98.9123304713" class="si-call"><span class="ts-icon"><i class="icon-call"></i></span><span class="ts-text" style="direction:ltr;font-size:13px;margin-top:0px;">+98.9123304713</span></a></li>
                                <li><a href="mailto:info@MyCMS.com" class="si-email3"><span class="ts-icon"><i class="icon-email3"></i></span><span class="ts-text" style="font-size:13px;margin-top:1px;">info@MyCMS.com</span></a></li>
                            </ul>
                        </div>
                        <!-- #top-social end -->

                    </div>

                </div>

            </div>
            <!-- #top-bar end -->
            <!-- Header
            ============================================= -->

            <header id="header" class="transparent-headers" data-sticky-class="not-dark">

                <div id="header-wrap">

                    <div class="container clearfix">

                        <div id="primary-menu-trigger"><i class="icon-reorder"></i></div>

                        <!-- Logo
                        ============================================= -->
                        <div id="logo">
                            <a href="/Home" class="standard-logo" data-dark-logo="/Content/images/logo-dark.png"><img src="/Content/images/logo.png" alt=""></a>
                            <a href="/Home" class="retina-logo" data-dark-logo="/Content/images/logo-dark.png"><img src="/Content/images/logo.png" alt=""></a>
                        </div>
                        <!-- #logo end -->
                        <!-- Primary Navigation
                        ============================================= -->

                            @Html.Action(MVC.Home.ActionNames.NavBar, MVC.Home.Name)

                        <!-- #primary-menu end -->                      

                    </div>

                </div>

            </header>
            <!-- #header end -->


            <div class="content" id="wrapper">
                
                <!-- end header div -->
                <!-- begin navbar div-->
                <div class="row-fluid " id="navbar">
                </div>
                <!-- end navbar div -->
                <div id="content-wrapper">
                    <div class="row-fluid" >
                        
                        @*@Html.Action(MVC.Home.ActionNames.Slider, MVC.Home.Name)*@
                        @RenderSection("Slider", required: false)

                        @RenderSection("AdvancedSearch", required: false)

                    </div>
                    <!-- end slider div -->
                    <div class="row-fluid" id="content" style="width:100%">
                        <!-- begin sidebar and announce div -->
                        @Html.Action(MVC.SideBar.ActionNames.Index, MVC.SideBar.Name)
                        <!-- end announce-wrapper div -->
                        <div class="span10 " id="main-content">
                            @RenderBody()
                        </div>
                        <div class="clear"></div>
                    </div>
                </div>
                <div id="loadingMessage">
                    <img src="@Links.Content.Images.loading4_gif" alt="loading..." />
                    <div>
                        <p>لطفا منتظر بمانید...</p>
                    </div>
                </div>
            </div>

            <!-- end sidebar and announce div -->
            <!-- end container div -->
            @*<footer>
                    <div id="footer">
                        <div id="footer-bar" style="text-align: center;">
                            <p style="margin: 0;"><span>با افتخار نیرو گرفته از MyCMS Blue</span></p>
                            <label class="label label-important">BETA VERSION</label>
                        </div>
                    </div>
                </footer>*@


            <!-- Footer
            ============================================= -->
            <footer id="footer" class="dark">

                <div class="container">

                    <!-- Footer Widgets
                    ============================================= -->
                    <div class="footer-widgets-wrap clearfix">

                        <div class="col_two_third">

                            <div class="col_one_third">

                                <div class="widget clearfix">

                                    <img src="/Content/images/footer-widget-logo.png" alt="" class="footer-logo">

                                    <p style="font-family:Yekan;font-size:13px;color: #fff;">گروه بازرگانی <strong>ماشین یاب</strong> ,مشاور وارائه کننده خدمات گمرکی  </p>

                                    <div style="background: url('/Content/images/world-map.png') no-repeat center center; background-size: 100%; display: block; min-height: 120px;">
                                        @Html.Action(MVC.Home.ActionNames.FooterContactInfo,MVC.Home.Name)
                                    </div>

                                </div>

                            </div>

                            <div class="col_one_third">

                                <div class="widget widget_links clearfix">

                                    <h4>پیوندهای مفید</h4>

                                    <ul>
                                        <li><a target="_blank" href="http://www.irica.gov.ir/">گمرک جمهوری اسلامی ایران</a></li>
                                        <li><a target="_blank" href="http://teh.mimt.gov.ir/">سازمان صنعت، معدن و تجارت استان تهران

                                        <li><a target="_blank" href="http://www.isca.ir/">انجمن علمی بازرگانی ایران</a></li>
                                        <li><a target="_blank" href="http://www.tpo.ir/">سازمان توسعه تجارت ایران</a></li>
                                        </a></li>
                                        <li><a target="_blank" href="http://www.rai.ir/">شركت راه آهن جمهوري اسلامي ايران</a></li>
                                        <li><a target="_blank" href="http://www.irisl.net/">شرکت کشتیرانی جمهوری اسلامی ایران</a></li>
                                        <li><a target="_blank" href="http://www.pmo.ir/">سازمان بنادر و دریانوردی</a></li>
                                    </ul>


                                </div>

                            </div>

                            <div class="col_one_third col_last">

                                <div class="widget clearfix">
                                    <h4>آخرین مطالب</h4>
                                    @Html.Action(MVC.Post.ActionNames.FooterLatestPost, MVC.Post.Name)                                    
                                </div>

                            </div>

                        </div>

                        <div class="col_one_third col_last">

                            <div class="widget" style="margin-bottom: -20px;">
                                <h4>با ما در ارتباط باشید</h4>
                            </div>
                            @Html.Action(MVC.Home.ActionNames.FooterSocialNetwork, MVC.Home.Name)

                        </div>

                    </div><!-- .footer-widgets-wrap end -->

                </div>

                <!-- Copyrights
                ============================================= -->
                <div id="copyrights">

                    <div class="container clearfix">

                        <div class="col_half" style="line-height: 2.6;">
                            1395&copy;  کلیه حقوق مادی و معنوی محفوظ است.
                        </div>

                        <div class="col_half col_last tleft">
                            <a href="http://www.machineyab.com" target="_blank"><img src="/Content/images/footer-sharosin.png" /></a>

                            <div class="clear"></div>

                        </div>

                    </div>

                </div><!-- #copyrights end -->

            </footer><!-- #footer end -->

        </div><!-- #wrapper end -->
        <!-- Go To Top
        ============================================= -->
        <div id="gotoTop" class="icon-angle-up"></div>

        <!-- Footer Scripts
        ============================================= -->
        <!-- begin logon modal form -->
        <div id="logOnModal" style="z-index: 99999999;" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        </div>
        <!-- end logon modal form -->
        @Scripts.Render("~/bundles/plugins")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/jqueryuitools")
        @Scripts.Render("~/bundles/sitejs")
        @Scripts.Render("~/bundles/functions")
        @RenderSection("scripts", false)
        @*@MiniProfiler.RenderIncludes()*@
    </body>
</html>
